[
{% for block in section.blocks %}
    {% if block.type == 'locator' %}
        {
            {% liquid  
            assign block_settings= block.settings
            assign block_collection= block_settings.collection
            assign id = block_collection.id
            assign title = block_collection.title
            assign alternate_title = block_settings.alternate_title
            assign url = block_collection.url
        %}
            "id": "{{id}}",
            "title": "{{alternate_title | default: title}}",
            "address": "{{block.settings.address}}",
            "url": "{{url}}"
        }
    {% endif %}
  {%- unless forloop.last %}, {% endunless %}
{% endfor %}
]